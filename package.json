{
  "name": "react-router-typescript-starter",
  "version": "1.0.0",
  "description": "React starter without the mess.",
  "main": "index.js",
  "scripts": {
    "start": "node www/server.js",
    "serve": "npm run beforeWatch && npm-run-all --parallel watch*",
    "build": "npm-run-all compile*",
    "compile-ts": "webpack -p --config webpack.config.server.js && webpack -p --config webpack.config.client.js",
    "compile-sass": "node-sass-chokidar src/style.scss www/public/style.css && cleancss -o www/public/style.css www/public/style.css",
    "beforeWatch": "node-sass-chokidar src/style.scss www/public/style.css && webpack --config webpack.config.server.js --mode=development && webpack --config webpack.config.client.js --mode=development",
    "watch-server": "webpack --config webpack.config.server.js --watch --mode=development",
    "watch-client": "webpack --config webpack.config.client.js --watch --mode=development",
    "watch-sass": "node-sass-chokidar src/style.scss www/public/style.css --watch",
    "watch-express": "cross-env NODE_ENV=development nodemon --inspect --ignore 'www/public/*' --watch 'www'  www/server.js",
    "watch-livereload": "livereload www -w 500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClickSimply/react-router-typescript-starter.git"
  },
  "keywords": [],
  "author": "Scott Lott <Scott@ClickSimply.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ClickSimply/react-router-typescript-starter/issues"
  },
  "homepage": "https://github.com/ClickSimply/react-router-typescript-starter#readme",
  "dependencies": {
    "express": "^4.13.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.0.36",
    "@types/node": "^10.3.3",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.14",
    "@types/react-router-dom": "^4.0.7",
    "clean-css-cli": "^4.1.11",
    "cross-env": "^5.2.0",
    "errorhandler": "^1.5.0",
    "livereload": "^0.7.0",
    "node-sass-chokidar": "^1.3.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2"
  }
}
